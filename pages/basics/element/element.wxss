.bg-mask1 view,
.bg-mask1 cover-view {
	z-index: 5;
	position: relative;
}
.bg-mask1 {
	background-color: #B4E1FF;
	position: relative;
}
.bg-mask1::after {
	content: "";
	border-radius: inherit;
	width: 100%;
	height: 100%;
	display: block;
	/* background-color: rgba(0, 0, 0, 0.4); */
  /* background-image: -webkit-linear-gradient(left,rgba(255,0,0,1) 0%,rgba(255,255,255,0) 50%); */
  background-image: #B4E1FF;
	position: absolute;

	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
}
