.scenePlan{position: relative;margin: 20rpx 30rpx 60rpx 30rpx;}
.sceneImg{
  width: 690rpx;
  height: 350rpx;border-radius: 10rpx; position: relative; 
   overflow:visible;
  /* background: url("") no-repeat center center; */
  background-size: 100% 100%;
   }
.sceneImg::before {
  content: "";
  border-radius: inherit;
	position: absolute;
  top: 8%;
  left: 0%;
  width: 100%;
  height: 100%;
  background: inherit;
  background-size: 100% 100%;
  z-index: -1;
  /* filter: blur(10rpx); */
  transform: scale(1, 1);
  filter: blur(10px) brightness(80%) opacity(.8);
}
.sceneMaks{position: absolute;
  height: 80rpx;
  width: 660rpx;
  background: rgba(0, 0, 0, 0.3);
  bottom: 0;left: 0;
  border-radius: 10rpx;
  color: #fff;
  line-height: 80rpx;padding-left: 30rpx;
}
