/* 背景图 */
.bg-img {
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
}
.bg-mask1 {
	background-color: var(--black);
	position: relative;
}
.mask{position:fixed; z-index:999; top:0; left:0; right:0; bottom:0; background-color:rgba(0,0,0,0.5);}
.bg-mask::after {
	content: "";
	border-radius: inherit;
	width: 100%;
	height: 100%;
	display: block;
	/* background-color: rgba(0, 0, 0, 0.4); */
  background-image: -webkit-linear-gradient(left,rgba(0,129,255,0.6) 0%,rgba(255,255,255,0) 100%);
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
}
.bg-mask view,
.bg-mask cover-view {
	z-index: 5;
	position: relative;
}
